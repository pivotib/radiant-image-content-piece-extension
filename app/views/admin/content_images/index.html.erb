<p><%= link_to "New Image", {:action => :new} %></p>
<table>
    <% @content_images.each do |content_image| %>
    <% end %>
    <% ContentImage.find(:all).each do |content_image| %>
      <tr class="node level-1">
        <td class="link">
          <%= link_to content_image.name, edit_admin_content_image_path(:id => content_image.id) %>
        </td>
        <td class="remove">
          <%= link_to image('remove', :alt => 'Remove link'), admin_content_image_path(:id => content_image.id), :method => :delete, :confirm => "are you sure?" %>
        </td>
      </tr>
    <% end %>
</table>